<?xml version="1.0" encoding="utf-8"?>
<start>
	<question q="Will it contain key/value pairs or values only?">
		<reponse r="Pairs">
			<question q="Is element order important?">
				<reponse r="yes">
					<question q="Insertion order or sorted by keys?">
						<reponse r="insertion ordered">
							<liste l="LinkedHashMap"/>
						</reponse>
						<reponse r="key sorted">
							<question q="Should it be synchronized?">
								<reponse r="yes">
									<liste l="HashTable"/>
								</reponse>
								<reponse r="non">
									<liste l="TreeMap"/>
								</reponse>
							</question>
						</reponse>
					</question>
				</reponse>
				<reponse r="no">
					<question q="Shout it be used as cache or store construction expensive object?">
						<reponse r="yes">
							<liste l="WeakHashMap"/>
						</reponse>
						<reponse r="no">
							<question q="Unique key to be determined using equals() instead of ==?">
								<reponse r="yes">
									<liste l="HashMap"/>
								</reponse>
								<reponse r="no">
									<liste l="IdentityHashMap"/>
								</reponse>
							</question>
						</reponse>
					</question>
				</reponse>
			</question>
		</reponse>
		<reponse r="Values only">
			<question q="Will it contain duplicate values?">
				<reponse r="no">
					<question q="Is element order important?">
						<reponse r="no">
							<liste l="HashSet"/>
						</reponse>
						<reponse r="yes">
							<question q="Insertion order or sorted by values?">
								<reponse r="insertion ordered">
									<liste l="LinkedHashSet"/>
								</reponse>
								<reponse r="value sorted">
									<liste l="TreeSet"/>
								</reponse>
							</question>
						</reponse>
					</question>
				</reponse>
				<reponse r="yes">
					<question q="Should it be synchronized?">
						<reponse r="yes">
							<question q="Should it act like LIFO stack?">
								<reponse r="yes">
									<liste l="Stack"/>
								</reponse>
								<reponse r="no">
									<liste l="Vector"/>
								</reponse>
							</question>
						</reponse>
						<reponse r="no">
							<question q="Fast random access required?">
								<reponse r="Yes">
									<liste l="ArrayList"/>
								</reponse>
								<reponse r="no">
									<question q="Fast sequential access,addition/deletion required?">
										<reponse r="no">
											<question r="Should it be used as a queue or double ended queue?">
												<reponse r="no">
													<liste l="ArrayList"/>
												</reponse>
												<reponse r="yes">
													<question q="Does it have to be fast performing queue?">
														<reponse r="yes">
															<liste l="ArrayDeque"/>
														</reponse>
														<reponse r="no">
															<liste l="LinkedList"/>
														</reponse>
													</question>
												</reponse>
											</question>
										</reponse>
										<reponse r="yes">
											<liste l="LinkedList"/>
										</reponse>
									</question>
								</reponse>
							</question>
						</reponse>
					</question>
				</reponse>
			</question>
		</reponse>
	</question>
</start>
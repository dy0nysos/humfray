			<reponse r="no" nextQ="none" listeid="3"/>
		</question>
		<question id="5" q="Insertion order or sorted by keys?">
			<reponse r="insertion ordered" nextQ="none" listeid="4"/>
			<reponse r="key sorted" nextQ="7"/>
		</question>
		<question id="7" q="should it be synchronized?">
			<reponse r="no" nextQ="none" listeid="5"/>
			<reponse r="yes" nextQ="none" listeid="6"/>
		</question>
		<question id="3" q="Will it contain duplicate values?">
			<reponse r="no" nextQ="8"/>
			<reponse r="yes" nextQ="10"/>

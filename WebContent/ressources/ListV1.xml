			<reponse r="yes" nextQ="none" listeid="10"/>
			<reponse r="no" nextQ="none" listeid="11"/>
		</question>
		<question id="12" q="Fast random access required?">
			<reponse r="yes" nextQ="none" listeid="12"/>
			<reponse r="no" nextQ="13"/>
		</question>
		<question id="13" q="Fast sequential access,addition/deletion required?">
			<reponse r="no" nextQ="14"/>
			<reponse r="yes" nextQ="none" listeid="13"/>
		</question>
		<question id="14" q="Should it be used as a queue or double-ended queue?">
			<reponse r="no" nextQ="none" listeid="12"/>
			<reponse r="yes" nextQ="15"/>
		</question>
		<question id="15" nextQ="Does it have to be fast performing queue?">
			<reponse r="yes" nextQ="none" listeid="14"/>
			<reponse r="no" nextQ="none" listeid="13"/>

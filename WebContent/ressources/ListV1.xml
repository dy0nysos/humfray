<?xml version="1.0" encoding="utf-8"?>
<start>
	<arrayOfQuestions>
		<question id="1" q="Will it contain key/value pairs or values only?">
			<reponse r="pairs" nextQ="2"/>
			<reponse r="values only" nextQ="3"/>
		</question>
		<question id="2" q="Is element order important?">
			<reponse r="no" nextQ="4"/>
			<reponse r="yes" nextQ="5"/>
		</question>
		<question id="4" q="Should it be used as cache or store construction expensive objects?">
			<reponse r="yes" nextQ="none" listeid="1"/>
			<reponse r="no" nextQ="6"/>
		</question>
		<question id="6" q="Unique key to be determined using equals instead of ==?">
